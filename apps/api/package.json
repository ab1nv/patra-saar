{
  "name": "@patrasaar/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "echo 'no lint configured'",
    "db:migrate:local": "wrangler d1 execute patrasaar-db --local --file=./src/db/schema.sql",
    "db:migrate:remote": "wrangler d1 execute patrasaar-db --remote --file=./src/db/schema.sql"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "@patrasaar/shared": "*",
    "better-auth": "^1.4.18",
    "hono": "^4.11.9",
    "kysely": "^0.28.11",
    "kysely-d1": "^0.4.0",
    "nanoid": "^5.1.6",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
